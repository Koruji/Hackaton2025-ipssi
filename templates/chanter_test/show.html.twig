{% extends 'base.html.twig' %}

{% block title %}Chantier{% endblock %}

{% block body %}
    <h1>Chantier</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ chantier.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ chantier.nom }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ chantier.adresse }}</td>
            </tr>
            <tr>
                <th>DebutTravaux</th>
                <td>{{ chantier.debutTravaux ? chantier.debutTravaux|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>FinTravaux</th>
                <td>{{ chantier.finTravaux ? chantier.finTravaux|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Status</th>
                <td>{{ chantier.status }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_chanter_test_index') }}">back to list</a>

    <a href="{{ path('app_chanter_test_edit', {'id': chantier.id}) }}">edit</a>

    {{ include('chanter_test/_delete_form.html.twig') }}
{% endblock %}
